{
    "window.titleBarStyle": "custom",
    "window.zoomLevel": 0,
    "editor.fontFamily": "Fira Code, Hack, 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "workbench.startupEditor": "none",
    "workbench.iconTheme": "vscode-icons",
    "editor.wordWrap": "wordWrapColumn",
    "editor.fontLigatures": true,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "workbench.settings.editor": "json",
    "explorer.sortOrder": "type",
    "terminal.integrated.fontSize": 12,
    "telemetry.enableTelemetry": false,
    "files.associations": {"*.m": "matlab"},
    "matlab.matlabpath": "C:/Program Files/MATLAB/R2018b/bin/matlab.exe",
    "matlab.mlintpath": "C:/Program Files/MATLAB/R2018b/bin/win64/mlint",
    "matlab-formatter.indentwidth": 2,
    "[markdown]": {
        "editor.wordWrap": "wordWrapColumn",
        "editor.quickSuggestions": false
    },
    "[latex]": {
        "editor.tabSize": 2,
        "editor.insertSpaces": true
    },
    "[matlab]": {
        "editor.tabSize": 2,
        "editor.insertSpaces": true
    },
    "workbench.colorTheme": "Ayu Mirage Bordered"
}
