{
  "editor.fontFamily": "'Fira Code Retina', 'Noto Mono', monospace",
    "workbench.startupEditor": "none",
    "editor.wordWrap": "wordWrapColumn",
    "editor.fontLigatures": true,
    "editor.rulers": [81],
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "workbench.settings.editor": "ui",
    "explorer.sortOrder": "type",
    "telemetry.enableTelemetry": false,
    "files.associations": {"*.m": "matlab"},
    "matlab.matlabpath": "C:/Program Files/MATLAB/R2019a/bin/matlab.exe",
    "matlab.mlintpath": "C:/Program Files/MATLAB/R2019a/bin/win64/mlint.exe",
    "matlab-formatter.indentwidth": 2,
    "[markdown]": {
        "editor.wordWrap": "wordWrapColumn",
        "editor.quickSuggestions": false
    },
    "[latex]": {
        "editor.tabSize": 2,
        "editor.insertSpaces": true
    },
    "[matlab]": {
        "editor.tabSize": 2,
        "editor.insertSpaces": true
    },
    "latex-workshop.latex.autoBuild.run": "never",
    "latex-workshop.latex.clean.fileTypes": [
      "*.aux",
      "*.bbl",
      "*.blg",
      "*.idx",
      "*.ind",
      "*.lof",
      "*.lot",
      "*.out",
      "*.toc",
      "*.acn",
      "*.acr",
      "*.alg",
      "*.glg",
      "*.glo",
      "*.gls",
      "*.ist",
      "*.fls",
      "*.log",
      "*.fdb_latexmk",
      "*.synctex.gz",
      "*.nav",
      "*.snm"
    ],
    "latex-workshop.latex.recipes": [{
        "name": "texify",
        "tools": [
          "texify"
        ]
      }],
      "latex-workshop.latex.tools": [{
        "name": "texify",
        "command": "texify",
        "args": [
          "--synctex",
          "--pdf",
          "--tex-option=\"-interaction=nonstopmode\"",
          "--tex-option=\"-file-line-error\"",
          "%DOC%.tex"
        ],
          "env": {}
      }
    ],
    "markdown.extension.italic.indicator": "_",
    "overtype.paste": true,
    "window.zoomLevel": -1,
    "latex-workshop.view.pdf.viewer": "tab",
    "editor.renderLineHighlight": "none",
    "workbench.colorTheme": "Bluloco Light",
    "r.rterm.windows": "C:\\Program Files\\Microsoft\\R Open\\R-3.5.3\\bin\\x64\\R.exe",
    "markdown.preview.fontSize": 16,
    "terminal.integrated.fontSize": 12
}
